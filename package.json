{
  "name": "ai-tweet-scheduler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:debug": "node test-runner.js",
    "test:debug:verbose": "node test-runner.js --verbose",
    "test:health": "node test-runner.js --endpoint=system-health",
    "test:queue": "node test-runner.js --endpoint=queue-validation",
    "test:realtime": "node test-runner.js --endpoint=queue-realtime",
    "test:edit": "node test-runner.js --endpoint=edit-simulation",
    "test:consistency": "node test-runner.js --endpoint=queue-consistency",
    "test:character": "node test-runner.js --endpoint=character-counting",
    "test:thread": "node test-runner.js --endpoint=thread-splitting",
    "test:format": "node test-runner.js --endpoint=content-formatting",
    "test:phase2": "node test-runner.js --endpoints=queue-realtime,edit-simulation,queue-consistency",
    "test:phase3": "node test-runner.js --endpoints=character-counting,thread-splitting,content-formatting"
  },
  "dependencies": {
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@types/react-datepicker": "^6.2.0",
    "@upstash/qstash": "^2.8.1",
    "next": "15.3.4",
    "openai": "^5.6.0",
    "react": "^19.0.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.0.0",
    "date-fns-tz": "^3.0.0",
    "twitter-api-v2": "^1.23.2",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
